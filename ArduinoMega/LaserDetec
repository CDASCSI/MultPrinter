bool LaserDetec( ) {
  digitalWrite(LASER_EMISSOR, HIGH);// liga laser
  MsgMidia(1);
  while (digitalRead(LASER_RECEPTOR) == HIGH){
     MOTOR_ELEVATOR.motorMove(HIGH);
    }
     digitalWrite(LASER_EMISSOR, LOW); // desliga Laser
     ELEVATOR_DISTANCE_CURRENT = Ultrasom();
     Serial.println(ELEVATOR_DISTANCE_CURRENT);
     MOTOR_ELEVATOR.motorStop();
     MOTOR_ELEVATOR.enable(LOW);
     MsgMidia(2);
  
  return true;
}
