void SetMotorx() {
  MOTOR_TREADMILL.enable(MOTOR_TREADMILL_ENABLE_LEVEL);
  MOTOR_TREADMILL.stepPerRound(MOTOR_TREADMILL_STEP_ROUND);
  MOTOR_TREADMILL.stepPerMm(STEP_MILLIMETER_MOTOR_TREADMILL);
  MOTOR_TREADMILL.motionConfig(ACCELERATION_MOTOR_TREADMILL, VELOCITY_MAXIMUM_MOTOR_TREADMILL, VELOCITY_MINIIMUM_MOTOR_TREADMILL);
  MOTOR_TREADMILL.endstopConfig(MOTOR_TREADMILL_LIMIT_MIN, MOTOR_TREADMILL_LIMIT_MAX, MOTOR_TREADMILL_LIMIT_LEVEL);

}
void SetMotory() {
  MOTOR_ELEVATOR.enable(MOTOR_ELEVATOR_ENABLE_LEVEL);
  MOTOR_ELEVATOR.stepPerRound(MOTOR_ELEVATOR_STEP_ROUND);
  MOTOR_ELEVATOR.stepPerMm(STEP_MILLIMETER_MOTOR_ELEVATOR);
  MOTOR_ELEVATOR.motionConfig(ACCELERATION_MOTOR_ELEVATOR, VELOCITY_MAXIMUM_MOTOR_ELEVATOR, VELOCITY_MINIIMUM_MOTOR_ELEVATOR);
  MOTOR_ELEVATOR.endstopConfig(MOTOR_ELEVATOR_LIMIT_MIN, MOTOR_ELEVATOR_LIMIT_MAX, MOTOR_ELEVATOR_LIMIT_LEVEL);
}
